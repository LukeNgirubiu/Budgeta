<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.budgeta">
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_budgeta"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_budgeta_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Budgeta">
        <activity
            android:name=".BudgetArchiveItems"
            android:exported="false" />
        <activity
            android:name=".creditArchive"
            android:exported="false" />
        <activity
            android:name=".budget_archive"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelpActivities.GraphHelp"
            android:exported="false" />
        <activity
            android:name=".HelpActivities.CashHelp"
            android:exported="false" />
        <activity
            android:name=".HelpActivities.CreditHelp"
            android:exported="false" />
        <activity
            android:name=".HelpActivities.BudgetHelp"
            android:exported="false" />
        <activity
            android:name=".Abouts"
            android:exported="false" />
        <activity
            android:name=".Help"
            android:exported="false" />
        <activity
            android:name=".Notifications"
            android:exported="false" />
        <activity android:name=".Present" />
        <activity android:name=".AnalyticCash" />
        <activity android:name=".AnalyticCredit" />
        <activity android:name=".CreditList" />
        <activity android:name=".AddCredit" />
        <activity android:name=".Credits" />
        <activity android:name=".DepositWithdraw" />
        <activity android:name=".AddUpdateBItem" />
        <activity android:name=".AddBudget" />
        <activity android:name=".Cash" />
        <activity android:name=".Deposit" />
        <activity android:name=".WithDraw" />
        <activity android:name=".BudgetItems" />
        <activity android:name=".Budgets" />
        <activity android:name=".Home" />
        <activity android:name=".Setting" />
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_path" />
        </provider>
    </application>

</manifest>